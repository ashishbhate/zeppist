{
  "configVersion": "v3",
  "app": {
    "appId": 26965,
    "appName": "Zeppist",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.1"
    },
    "icon": "icon.png",
    "vender": "github.com/ashishbhate",
    "description": "Sync Todoist Tasks for today and notify when due"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage",
    "device:os.alarm"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "2.0.0",
      "target": "2.0.0",
      "minVersion": "2.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/index",
            "page/alarm"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "st": "r"
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Zeppist"
    }
  },
  "defaultLanguage": "en-US"
}
